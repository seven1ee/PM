<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lll.po.Emp">
    <select id="getEmp" resultType="lll.po.Emp">
        SELECT * FROM emp e  where  e.empid = 1
    </select>
</mapper>